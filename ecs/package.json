  "description": "Architecture and systemisation experiment. Source: https://www.youtube.com/watch?v=GFO_txvwK_c",
  "main": "Main.js",
  "scripts": {
    "watch": "tsc --watch",
    "watch:assets": "chokidar 'src/index.html' 'src/style.css' 'assets/**/*' -c 'node copy-static-assets.js'",
